{
  "name": "@gnosis.pm/gnosisjs",
  "version": "0.2.0",
  "description": "A javascript library for building applications on top of Gnosis, the Ethereum prediction market platform",
  "scripts": {
    "lint": "eslint ./",
    "compile": "cd node_modules/@gnosis.pm/gnosis-core-contracts && truffle compile",
    "migrate": "cd node_modules/@gnosis.pm/gnosis-core-contracts && truffle migrate",
    "netclean": "cd node_modules/@gnosis.pm/gnosis-core-contracts && truffle networks --clean",
    "buildcontracts": "cd node_modules/@gnosis.pm/gnosis-core-contracts && truffle build",
    "build": "npm run netclean && webpack",
    "dev": "npm run migrate && webpack-dev-server --watch-poll",
    "test": "mocha --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gnosis/gnosis.js.git"
  },
  "keywords": [
    "gnosis",
    "ethereum",
    "blockchain",
    "contract",
    "library",
    "prediction",
    "market"
  ],
  "author": "Gnosis (https://gnosis.pm/)",
  "license": "MIT",
  "homepage": "https://github.com/gnosis/gnosis.js#readme",
  "devDependencies": {
    "@gnosis.pm/gnosis-core-contracts": "github:gnosis/gnosis-contracts-truffle",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-register": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-plugin-babel": "^4.1.1",
    "ipfs-mini": "^1.1.2",
    "json-loader": "^0.5.4",
    "mocha": "^3.4.2",
    "style-loader": "^0.18.2",
    "truffle": "^3.2.5",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.5.0",
    "decimal.js": "^7.2.1",
    "jsdoc": "^3.4.3",
    "lodash": "^4.17.4"
  }
}
